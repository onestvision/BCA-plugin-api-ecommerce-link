{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string"
    },
    "identify_number": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^\\d+$"
    },
    "identification_type": {
      "type": "string"
    },
    "phone_number": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "email": {
      "type": "email"
    },
    "razon_social": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "payments"
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "payment"
    }
  }
}
